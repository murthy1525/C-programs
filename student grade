#include<stdio.h>
int main()
{
    float phy,mat,bio,che,com;
    float total,percentage;
    int g;
    printf("Enter Physics marks: ");
    scanf("%f",&phy);
    printf("Enter Mathematics marks: ");
    scanf("%f",&mat);
    printf("Enter Biology marks: ");
    scanf("%f",&bio);
    printf("Enter Chemistry marks: ");
    scanf("%f",&che);
    printf("Enter Computers marks: ");
    scanf("%f",&com);
    
     total=(phy+mat+bio+che+com);
     percentage=(total/500)*100;
      g=(int)(percentage);
    switch(g)
    {
        case 90 ... 100:
        printf("Grade A");
        break;
        case 80 ... 89:
        printf("Grade B");
        break;
        case 70 ... 79:
        printf("Grade C");
        break;
        case 60 ... 69:
        printf("Grade D");
        break;
        case 40 ... 59:
        printf("Grade E");
        break;
        case 0 ... 39:
        printf("Grade F");
        break;
        default:
        printf("Fail");
        break;
    }
    return 0;
}
