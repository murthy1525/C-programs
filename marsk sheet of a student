#include <stdio.h>
int main()
{
   int num,tel,hin,eng,mat,sci,soc,t;
   float avg;
   char nam[10];
   printf("Enter student roll no: ");
   scanf("%d",&num);
   printf("Enter student name: ");
   scanf("%s",nam);
   printf("Enter marks\n");
   scanf("%d%d%d%d%d%d\n",&tel,&hin,&eng,&mat,&sci,&soc);
   t=tel+hin+eng+mat+sci+soc;
   avg=t/6.0;
   printf("--------------------------------------------------\n");
   printf("\tBOARD OF SECONDARY EDUCATION\n");
   printf("\t\tANDHRA PRADESH\n");
   printf("--------------------------------------------------\n");
   printf("Roll no: %d \t Name: %s\n",num,nam);
   printf("--------------------------------------------------\n");
   printf("sub\tMarks\tPass/Fail\n");
   printf("--------------------------------------------------\n");
   printf("Tel\t%d\t",tel);
   if(tel>=35)
   {
       printf("P\n");
   }
   else
   {
       printf("F\n");
   }
   printf("Hin\t%d\t",hin);
   if(hin>=35)
   {
       printf("P\n");
   }
   else
   {
       printf("F\n");
   }printf("Eng\t%d\t",eng);
   if(eng>=35)
   {
       printf("P\n");
   }
   else
   {
       printf("F\n");
   }printf("Mat\t%d\t",mat);
   if(mat>=35)
   {
       printf("P\n");
   }
   else 
   {
       printf("F\n");
   }printf("Tel\t%d\t",tel);
   if(tel>=35)
   {
       printf("P\n");
   }
   else
   {
       printf("F\n");
   }printf("Sci\t%d\t",sci);
   if(sci>=35)
   {
       printf("P\n");
   }
   else
   {
       printf("F\n");
   }printf("Soc\t%d\t",soc);
   if(soc>=35)
   {
       printf("P\n");
   }
   else
   {
       printf("F\n");
   }
   printf("------------------------------------------------\n");
   printf("%s total=%d, Avg=%f and got ",nam,t,avg);
   if(avg>=75)
   {
       printf("Distinction");
   }
   else if(avg>=60)
   {
       printf("1st class");
   }
   else if("avg>=50")
   {
       printf("2nd class");
   }
   else 
   {
       printf("3rd clsss");
   }
   return 0;
}
